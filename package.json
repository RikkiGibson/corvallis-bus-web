  "description": "A web app for finding out when the bus is going to arrive.",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass_dev": "sass src/css/index.scss | postcss --use autoprefixer > build/css/index.css",
